#include <bits/stdc++.h>
using namespace std;

int main() {
    vector<pair<int,int>> act = { {1,3}, {2,5}, {4,6}, {6,7}, {5,9}, {8,9} };
    sort(act.begin(), act.end(), [](auto &a, auto &b){
        return a.second < b.second;           
    });

    int last = -1;
    cout << "Selected activities:\n";
    for(auto &p : act) {
        if(p.first >= last) {                 
            cout << "(" << p.first << "," << p.second << ")\n";
            last = p.second;
        }
    }
}
