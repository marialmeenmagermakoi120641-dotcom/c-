#include <bits/stdc++.h>
using namespace std;

int linearSearch(vector<int>& arr, int key) {
    for (int i = 0; i < arr.size(); i++) {
        if (arr[i] == key) {
            return i;  
        }
    }
    return -1; 
}

int main() {
    vector<int> arr = {10, 23, 45, 70, 11, 15};
    int key = 70;

    int index = linearSearch(arr, key);

    if (index != -1)
        cout << "Element found at index: " << index << endl;
    else
        cout << "Element not found!" << endl;

    return 0;
}
